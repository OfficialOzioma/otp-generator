{
    "description": "Laravel OTP generator and validation",
    "homepage": "https://github.com/officialOzioma/OTP-generator",
    "keywords": [
        "laravel",
        "otp",
        "one time passwords",
        "one time pin"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Ozioma Dike",
            "email": "dikep15@gmail.com"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Ozioma\\Otp\\OtpServiceProvider"
            ],
            "aliases": {
                "Otp": "Ozioma\\Otp\\OtpFacade"
            }
        }
    },
    "require": {
        "php": "^5.5.9|^7.0|^8.0",
        "illuminate/support": "^5.2|^6.0|^7.0|^8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "Ozioma\\Otp\\": "src"
        }
    }
}
